{
    "name": "gemy/mygift-api",
    "description": "using for mygift-api",
    "type": "library",
    "version": "1.0.1",
    "require": {
        "php": ">=7.0",
        "laravel/framework": ">=7.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Gemy\\MygiftApi\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "app/",
            "Gemy\\MygiftApi\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "authors": [
        {
            "name": "mohamed gamal",
            "email": "mohamedgammal55@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Gemy\\MygiftApi\\MyGiftRouteServiceProvider"
            ],
            "aliases": {
                "MyGiftApi": "Gemy\\MygiftApi\\MyGiftApi"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
